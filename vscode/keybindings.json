// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "cmd+'",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "editor.action.insertCursorBelow",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "editor.action.insertCursorAbove",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+t",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "cmd+u",
        "command": "editor.action.transformToUppercase"
    },
    {
        "key": "cmd+l",
        "command": "editor.action.transformToLowercase"
    },
    {
        "key": "cmd+r",
        "command": "clojureVSCode.eval"
    },
    {
        "key": "shift+cmd+r",
        "command": "clojureVSCode.evalAndShowResult"
    },
    {
        "key": "alt+cmd+m",
        "command": "editor.emmet.action.matchTag"
    },
    {
        "key": "ctrl+b",
        "command": "editor.action.goToDeclaration",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "f12",
        "command": "-editor.action.goToDeclaration",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "ctrl+b",
        "command": "-cursorLeft",
        "when": "textInputFocus"
    },
    {
        "key": "shift+alt+7",
        "command": "-fsi.SendLine",
        "when": "editorFocus && !findWidgetVisible && editorLangId == 'fsharp'"
    },
    {
        "key": "shift+alt+7",
        "command": "-elm.replSendLine",
        "when": "editorFocus && !findWidgetVisible && editorLangId == 'elm'"
    }
]
